{
  "name": "Color Picker Ez",
  "version": "0.0.1",
  "manifest_version": 3,
  
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/logo-16.png",
      "48": "images/logo-48.png",
      "128": "images/logo-128.png"
    }
  },
  "icons": {
    "16": "images/logo-16.png",
    "48": "images/logo-48.png",
    "128": "images/logo-128.png"
  },
  "permissions": ["activeTab"]
}
